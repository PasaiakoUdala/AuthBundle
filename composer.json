{
  "type": "symfony-bundle",
  "name": "pasaikoudala/authbundle",
  "description": "Pasaiako Udala Authentication against LDAP",
  "license": "MIT",
  "authors": [
    {
      "name": "Iker Ibarguren",
      "email": "iibarguren@pasaia.net"
    }
  ],
  "require": {
    "php": "^7.4.0",
    "doctrine/doctrine-bundle": "^2.4",
    "doctrine/orm": "^2.9",
    "symfony/dotenv": "5.3.*",
    "symfony/flex": "^1.3.1",
    "symfony/ldap": "5.3.*",
    "symfony/security-bundle": "5.3.*",
    "symfony/twig-bundle": "5.3.*",
    "symfony/yaml": "5.3.*"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "PasaiakoUdala\\AuthBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PasaiakoUdala\\AuthBunle\\Tests\\": "tests/"
    }
  }
}
